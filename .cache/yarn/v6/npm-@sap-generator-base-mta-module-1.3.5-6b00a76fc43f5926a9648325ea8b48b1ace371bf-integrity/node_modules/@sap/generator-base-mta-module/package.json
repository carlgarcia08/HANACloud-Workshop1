{
  "name": "@sap/generator-base-mta-module",
  "version": "1.3.5",
  "displayName": "Multitarget Application Module Base Generator",
  "author": "SAP SE",
  "description": "Generator for collecting user inputs on multitarget application module",
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "scripts": {
    "build": "npm run lint && npm run compile && npm run test",
    "compile": "tsc -p .",
    "clean": "rimraf ./generators",
    "watch": "tsc -p . --watch",
    "lint": "eslint . --ext .ts --fix --max-warnings=0",
    "test": "nyc ts-mocha -p tsconfig.json test/**/*.spec.ts --opts=./mocha.opts"
  },
  "keywords": [
    "yeoman-generator"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@types/mocha": "^5.2.6",
    "@types/rimraf": "^2.0.2",
    "@types/sinon": "7.0.13",
    "@types/tmp": "^0.1.0",
    "@types/yeoman-test": "^2.0.2",
    "@types/yeoman-assert": "3.1.1",
    "@typescript-eslint/parser": "^3.7.0",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@types/yeoman-generator": "3.1.4",
    "@types/inquirer": "7.3.1",
    "eslint": "6.8.0",
    "mocha": "^6.1.4",
    "nyc": "15.1.0",
    "rimraf": "^2.6.3",
    "sinon": "8.1.1",
    "source-map-support": "^0.5.12",
    "tmp": "^0.1.0",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.1.0",
    "typescript": "3.9.7",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0",
    "@sap-devx/yeoman-ui-types": "0.0.1",
    "eslint-plugin-eslint-comments": "^3.2.0"
  },
  "dependencies": {
    "chalk": "2.4.2",
    "@sap/mta-lib": "1.6.7",
    "yeoman-generator": "4.10.1"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
