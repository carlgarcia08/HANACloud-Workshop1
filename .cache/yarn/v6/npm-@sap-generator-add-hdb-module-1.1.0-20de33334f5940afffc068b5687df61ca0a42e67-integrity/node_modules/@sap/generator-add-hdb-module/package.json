{
  "name": "@sap/generator-add-hdb-module",
  "displayName": "SAP HANA Database Module",
  "version": "1.1.0",
  "description": "A database module contains design-time database artifacts. On deployment, a database module is assigned an HDI container to which the corresponding run-time database objects are deployed.",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://help.sap.com/viewer/c2b99f19e9264c4d9ae9221b22f6f589/",
  "scripts": {
    "test": "grunt unit",
    "unittest": "grunt unit && nyc check-coverage -t ./coverage/unit --lines 80 --functions 80",
    "componenttest": "grunt component && nyc check-coverage -t ./coverage/component-${HANA_TOOLING_TEST_MODE:-server} --lines 0 --functions 0",
    "uicomponenttest": "grunt component-ui",
    "checkcoverage": "grunt checkcoverage && nyc check-coverage -t ./coverage/combined --lines 80 --functions 80",
    "checkstyle": "eslint-output --ext .js,.json --max-warnings 0 .",
    "lint": "eslint --ext .js,.json --max-warnings 0 -f stylish .",
    "prepare": "grunt checkShrinkwrap"
  },
  "main": "generators/app/index.js",
  "files": [
    "generators",
    "yeoman.png",
    "features.json"
  ],
  "keywords": [
    "yeoman-generator",
    "hana-tooling"
  ],
  "devDependencies": {
    "chai": "4.2.0",
    "eslint": "7.8.1",
    "eslint-output": "2.0.3",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-mocha": "8.0.0",
    "fs-extra": "9.0.1",
    "grunt": "1.3.0",
    "grunt-cli": "1.3.2",
    "grunt-continue": "0.1.0",
    "grunt-contrib-clean": "2.0.0",
    "grunt-env": "1.0.1",
    "grunt-express-server": "0.5.4",
    "grunt-istanbul-combine": "0.1.0",
    "grunt-karma": "4.0.0",
    "grunt-mkdir": "1.0.0",
    "grunt-mocha-nyc": "1.0.3",
    "grunt-mocha-test": "0.13.3",
    "gruntify-eslint": "5.0.0",
    "husky": "4.3.0",
    "karma": "5.2.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-junit-reporter": "2.0.1",
    "karma-qunit": "4.1.1",
    "karma-requirejs": "1.1.0",
    "karma-webdriver-launcher": "1.0.8",
    "mocha": "8.1.3",
    "mocha-jenkins-reporter": "0.4.5",
    "nyc": "15.1.0",
    "qunit": "2.11.1",
    "requirejs": "2.3.6",
    "selenium-webdriver": "4.0.0-alpha.7",
    "sinon": "9.0.3",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^3.0.0"
  },
  "dependencies": {
    "@sap-devx/inquirer-gui-folder-browser-plugin": "0.0.3",
    "@sap-devx/yeoman-ui-types": "1.0.2",
    "@sap/hana-tooling-feature-toggles": "1.0.0",
    "@sap/cf-tools": "0.8.1",
    "command-exists": "1.2.9",
    "@sap/generator-base-mta-module": "1.3.5",
    "@sap/mta-lib": "1.6.7",
    "rimraf": "3.0.2",
    "uuid": "8.3.2",
    "yeoman-generator": "4.13.0",
    "md5": "2.3.0"
  },
  "generator-filter": {
    "types": [
      "mta.module"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "grunt checkShrinkwrap"
    }
  }
}
